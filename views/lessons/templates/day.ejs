<%_ for(let index = 0; index < Math.max(lessons.meta.max, 5); index++ ) { -%>
    <%_ let lesson = lessons[index] -%>
    <%_ if (lesson){ -%>
        <%_ %><b>[<%=index+1%>]</b> <b><%=!params.hideTime ? `[${localizer.interval(index)}] `:''%></b><u><%= lesson.cabinets.join(', ') %></u><%= lesson.cabinets.length ? ' · ' : '' %><i><%=lesson.type.join(', ')%></i> - <%= lesson.name.join('. ') %>
        <%_ if (!params.hideTeachers) { -%>
            <%_ lesson.teachers.forEach(teacher => { -%>
                <%_ %> ≻  <i><%=teacher%></i>
            <%_ }) -%>
        <%_ } -%>
    <%_ } else if (!skipEmpty) { -%>
        <%_ %><b>[<%=index+1%>]</b> <b><%=!params.hideTime ? `[${localizer.interval(index)}]`:''%></b> <i>Нічого</i>
    <%_ } -%>
<%_ } -%>